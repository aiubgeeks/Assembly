.MODEL SMALL
.STACK 100H
.DATA
    CHAR DB ?
.CODE

MAIN PROC      
    MOV AH,2      ;OUTPUT ?
    MOV DL,'?'
    INT 21H
    
    MOV AH,1      ;INPUT FIRST CHARACTER
    INT 21H       ;SAVE THE CHARACTER TO BL
    MOV BL,AL     
    INT 21H       ;INPUT SECOND CHARACTER
    MOV CHAR,AL   ;SAVE TO CHAR
    
    MOV AH,2      ;SET FOR OUTPUT
    MOV DL,0DH    ;OUTPUT CARRIAGE RETURN
    INT 21H
    MOV DL,0AH    ;LINE FEED
    INT 21H
    
    CMP CHAR,BL   ;COMPARE BL WITH CHAR
    JG  ELSE      ;JUMP IF CHAR > BL
    MOV DL,CHAR   ;OUTPUT CHAR (SMALLER THAN BL)
    INT 21H
    MOV DL,BL     ;OUTPUT BL (GREATER THAN CHAR)
    INT 21H
    JMP END_IF
 
 ELSE:
    MOV DL,BL     ;OUTPUT BL (SMALLER THAN BL)
    INT 21H
    MOV DL,CHAR   ;OUTPUT CHAR (GREATER THAN CHAR)
    INT 21H
    
 END_IF:          ;RETURN TO DOS
    MOV AH,4CH
    INT 21H   
    
    
    MAIN ENDP
 END MAIN
